<h1>Listing licenses</h1>

<table>
  <tr>
  <% for column in License.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for license in @licenses %>
  <tr>
  <% for column in License.content_columns %>
    <td><%=h license.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => license %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => license %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => license }, :confirm => 'Are you sure?', :method => :post %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @license_pages.current.previous } if @license_pages.current.previous %>
<%= link_to 'Next page', { :page => @license_pages.current.next } if @license_pages.current.next %> 

<br />

<%= link_to 'New license', :action => 'new' %>
